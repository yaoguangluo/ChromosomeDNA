package jniLYGAFDCDFFT;

/*
 * 著作权人, 作者 罗瑶光, 浏阳
 * yaoguangluo@outlook.com, 313699483@qq.com, 2080315360@qq.com, lygtin@gmail.com
 * 15116110525, 19173129338, 16674208794
 * 430181198505250014, G24402609, EB0581342
 * 204925063, 389418686, F2406501, 0626136
 * 湖南省 浏阳市 集里街道 神仙坳社区 大塘冲路 208号 阳光家园别墅小区 第十栋
 * */
//这个算法纪念罗瑶光先生逝去的青春。
public class LYGAFDCTDFFT {
    public double[] jniLYGAFLOGDFT(double[] input) {
        //载入过程
        out = log3ffdit(cps);
        //时能函数计算
        for (int j = 0; j < input.length; j++) {
            output[j] =  math.fabs(out[j].b * out[j].cpi - (2 * out[j].ca * out[j].cb * out[j].b* out[j].cpi));
        }
        return output;
    }
    public cp[] log3ffdit(cp[] cps) {
        //罗瑶光fft蝶形过程
        for (int j = 0; j < n / 2; j++) {  
            。。。
            。。。
            double kii = i * ccc - c * iii;
            。。。
            。。。
            output[j].cpi = ab[j].cpi + kii;
            output[j].ca = ab[j].ca + ccc ;
            output[j].cb = ab[j].cb + c;
            output[j + n / 2].cpi = abc[j].cpi - kii;
            output[j + n / 2].ca = abc[j].ca - ccc;
            output[j + n / 2].cb = abc[j].cb - c;
            。。。
        }
}


